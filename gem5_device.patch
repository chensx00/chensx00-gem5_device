+                void recvFunctional(PacketPtr pkt) override {
+                    panic("recvFunctional unimpl.\n");  
+                }
+
+                bool recvTimingReq(PacketPtr pkt) override ;
+
+                void recvRespRetry() override ;
+   
+        };
+
+

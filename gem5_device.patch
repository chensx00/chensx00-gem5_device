+
+void 
+SimpleDeviceObj::sendResponse()
+{
+    DPRINTF(Device_Obj,"sendResponse, addr = %" PRIx64 ", is %s\n",readyToRespPkt->getAddr(),readyToRespPkt->isRead()?"Read":"Write");
+
+    devicePort.sendPacket(readyToRespPkt);
+}

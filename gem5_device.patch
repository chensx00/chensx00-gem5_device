+
+        outputSimDev out;
+        
+        uint8_t ptrData;
+
+        uint16_t requestorID;
+
+        int requestCnt;
+
+        enum DeviceStatus {
+            IDEL,   //waiting enable signal
+            RTLRun, //input data to RTL model to run
+            Waiting,   //waiting for RTL model
+            SetOK  //set complete signal
+
+        };
+
+        DeviceStatus status;
+    
+
+        uint8_t deviceReg[addressNum];
+
+        EventFunctionWrapper event;
+
